<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:complexType name="contextAttribute">
      <xsd:complexContent>
         <xsd:extension base="stringAttribute">
            <xsd:sequence/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="stringAttribute">
      <xsd:complexContent>
         <xsd:extension base="parameterizedAttribute">
            <xsd:sequence/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="parameterizedAttribute" abstract="true">
      <xsd:complexContent>
         <xsd:extension base="attribute">
            <xsd:sequence/>
            <xsd:attribute name="variableName" type="xsd:string"/>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="attribute" abstract="true">
      <xsd:sequence/>
      <xsd:attribute name="key" type="xsd:string"/>
   </xsd:complexType>
   <xsd:element name="contextAttribute" type="contextAttribute"/>
   <xsd:element name="stringAttribute" type="stringAttribute"/>
   <xsd:element name="parameterizedAttribute" type="parameterizedAttribute"/>
   <xsd:element name="attribute" type="attribute"/>
   <xsd:simpleType name="stringValue">
      <xsd:restriction base="xsd:string"/>
   </xsd:simpleType>
</xsd:schema>
